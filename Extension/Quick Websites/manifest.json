{
    "manifest_version": 2,

    "name": "Quick Websites",
    "description": "Quickly launch Useful Websites",
    "version": "1.0.0",
    "icons": { "128": "icon.png" },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}